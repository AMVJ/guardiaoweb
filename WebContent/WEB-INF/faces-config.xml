<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>pessoaMB</managed-bean-name>
  <managed-bean-class>br.com.guardiao.mb.PessoaMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>usuarioMB</managed-bean-name>
  <managed-bean-class>br.com.guardiao.mb.UsuarioMB</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>ftpMB</managed-bean-name>
  <managed-bean-class>br.com.guardiao.ftp.FTPConnect</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>camera</property-name>
   <property-class>br.com.guardiao.modelo.Camera</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>progressBarBean</managed-bean-name>
  <managed-bean-class>br.com.guardiao.mb.ProgressBarBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>cameraMB</managed-bean-name>
  <managed-bean-class>br.com.guardiao.mb.CameraMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>pages/index</display-name>
  <from-view-id>/pages/index.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toConectarFtp</from-outcome>
   <to-view-id>/pages/ftp/conectarFTP.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/index</display-name>
  <from-view-id>/pages/index.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toCadastraUsuario</from-outcome>
   <to-view-id>/pages/cadastroUsuario/cadastraUsuario.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/index</display-name>
  <from-view-id>/pages/index.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toCadastraCamera</from-outcome>
   <to-view-id>/pages/cadastroCamera/cadastraCamera.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/index</display-name>
  <from-view-id>/pages/index.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toAtualizaPessoa</from-outcome>
   <to-view-id>/pages/cadastroPessoa/atualizaPessoa.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/index</display-name>
  <from-view-id>/pages/index.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toCadastraPessoa</from-outcome>
   <to-view-id>/pages/cadastroPessoa/cadastraPessoa.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/index</display-name>
  <from-view-id>/pages/index.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toPesquisaPessoa</from-outcome>
   <to-view-id>/pages/cadastroPessoa/pesquisaPessoa.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/index</display-name>
  <from-view-id>/pages/index.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toListagemCamera</from-outcome>
   <to-view-id>/pages/cadastroCamera/listagemCameras.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/index</display-name>
  <from-view-id>/pages/index.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toRemovePessoa</from-outcome>
   <to-view-id>/pages/cadastroPessoa/removePessoa.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/cadastroPessoa/atualizaPessoa</display-name>
  <from-view-id>/pages/cadastroPessoa/atualizaPessoa.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toIndex</from-outcome>
   <to-view-id>/pages/index.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/cadastroPessoa/cadastraPessoa</display-name>
  <from-view-id>/pages/cadastroPessoa/cadastraPessoa.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toIndex</from-outcome>
   <to-view-id>/pages/index.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/cadastroPessoa/pesquisaPessoa</display-name>
  <from-view-id>/pages/cadastroPessoa/pesquisaPessoa.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toIndex</from-outcome>
   <to-view-id>/pages/index.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>pages/cadastroPessoa/removePessoa</display-name>
  <from-view-id>/pages/cadastroPessoa/removePessoa.jsp</from-view-id>
  <navigation-case>
   <from-outcome>toIndex</from-outcome>
   <to-view-id>/pages/index.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <validator>
  <validator-id>emailValidator</validator-id>
  <validator-class>br.com.guardiao.validador.EmailValidator</validator-class>
 </validator>
</faces-config>
